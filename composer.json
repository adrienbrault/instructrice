{
    "name": "adrienbrault/instructrice",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AdrienBrault\\Instructrice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdrienBrault\\Instructrice\\Tests\\": "tests/",
            "Demo\\": "demo/"
        }
    },
    "authors": [
        {
            "name": "Adrien Brault",
            "email": "adrien.brault@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^6.0 | ^7.0",
        "symfony/console": "^6.0 | 7.0",
        "phpunit/phpunit": "^11.1",
        "phpstan/phpstan": "^1.10",
        "symplify/easy-coding-standard": "*",
        "monolog/monolog": "^3.5",
        "symfony/monolog-bridge": "^7.0"
    },
    "require": {
        "php": "^8.1",
        "azjezz/psl": "^2.0",
        "gioni06/gpt3-tokenizer": "^1.2",
        "greghunt/partial-json": "^1.1",
        "guzzlehttp/guzzle": "^7.0",
        "api-platform/json-schema": "^3.0",
        "phpdocumentor/type-resolver": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "symfony/property-access": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
